{
  "name": "@singular/core",
  "version": "1.0.0-beta.5",
  "description": "Singular framework core module",
  "main": "dist/core.js",
  "scripts": {
    "start": "node ./node_modules/rimraf/bin.js \"./dist\" && ./node_modules/typescript/bin/tsc --build",
    "docs": "node ./node_modules/rimraf/bin.js \"./docs\" && ./node_modules/typedoc/bin/typedoc --out docs src",
    "test": "node ./node_modules/rimraf/bin.js \"./test/dist\" && ./node_modules/typescript/bin/tsc -p test/tsconfig.json && ./node_modules/mocha/bin/mocha ./test/dist/main.spec.js",
    "e2e": "npm run build-e2e-server && npm run build-e2e-tests && npm run gen-cert && npm run copy-e2e-assets && npm run run-e2e-tests",
    "build-e2e-server": "echo \"Building the E2E server...\" && node ./node_modules/rimraf/bin.js \"./e2e/server/dist\" && ./node_modules/typescript/bin/tsc -p e2e/server/tsconfig.json",
    "build-e2e-tests": "echo \"Building the E2E tests...\" && node ./node_modules/rimraf/bin.js \"./e2e/dist\" && ./node_modules/typescript/bin/tsc -p e2e/tsconfig.json",
    "gen-cert": "cd ./e2e/server/src/credentials && openssl req -x509 -newkey rsa:4096 -keyout ./key.pem -out ./cert.pem -nodes -subj '/CN=localhost' -days 365",
    "run-e2e-tests": "echo \"Running the E2E tests...\" && ./node_modules/mocha/bin/mocha ./e2e/dist/main.spec.js",
    "copy-e2e-assets": "echo \"Copying E2E assets...\" && ./node_modules/copyfiles/copyfiles -f \"./e2e/server/src/credentials/*.pem\" ./e2e/server/dist/credentials && ./node_modules/copyfiles/copyfiles -f \"./e2e/src/data/*.json\" ./e2e/dist/data"
  },
  "files": [
    "dist"
  ],
  "repository": {
    "type": "git",
    "url": "git+ssh://git@github.com/singularframework/core.git"
  },
  "keywords": [
    "singular",
    "framework",
    "nodejs",
    "node",
    "typescript",
    "backend",
    "server",
    "express",
    "http",
    "validation"
  ],
  "author": "Ramtin Soltani <ramtin@chiselpowered.com>",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/singularframework/core/issues"
  },
  "homepage": "https://github.com/singularframework/core#readme",
  "dependencies": {
    "@singular/common": "^1.0.0-beta.2",
    "body-parser": "^1.19.0",
    "callsites": "^3.1.0",
    "chalk": "^4.1.0",
    "cookie-parser": "^1.4.5",
    "cors": "^2.8.5",
    "express": "^4.17.1",
    "fs-extra": "^9.0.1",
    "lodash": "^4.17.20",
    "luxon": "^1.25.0",
    "micromatch": "^4.0.2",
    "source-map-support": "^0.5.19",
    "strip-ansi": "^6.0.0",
    "tsconfig-paths": "^3.9.0"
  },
  "devDependencies": {
    "@singular/pipes": "^1.0.0-beta.2",
    "@singular/validators": "^1.0.0-beta.3",
    "@types/bcrypt": "^3.0.0",
    "@types/body-parser": "^1.19.0",
    "@types/chai": "^4.2.12",
    "@types/cookie-parser": "^1.4.2",
    "@types/cors": "^2.8.7",
    "@types/express": "^4.17.7",
    "@types/fast-levenshtein": "0.0.1",
    "@types/fs-extra": "^9.0.1",
    "@types/got": "^9.6.11",
    "@types/jsonwebtoken": "^8.5.0",
    "@types/lodash": "^4.14.161",
    "@types/luxon": "^1.24.4",
    "@types/micromatch": "^4.0.1",
    "@types/mocha": "^8.0.3",
    "@types/node": "^14.6.0",
    "@types/source-map-support": "^0.5.3",
    "bcrypt": "^5.0.0",
    "chai": "^4.2.0",
    "copyfiles": "^2.3.0",
    "fast-levenshtein": "^3.0.0",
    "got": "^11.6.1",
    "jsonwebtoken": "^8.5.1",
    "mocha": "^8.1.2",
    "rimraf": "^3.0.2",
    "typedoc": "^0.18.0",
    "typescript": "^4.0.2"
  }
}
